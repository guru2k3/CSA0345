#include<stdio.h>
#include<math.h>
void insertionsort(int n,int arr[])
{
	int i,key,j;
	for(i=1;i<n;i++)
	{
	key=arr[i];
	j=i-1;
	while(j>=0 && arr[j]>key)
	{
		arr[j+1]=arr[j];
		j=j-1;
	}
	arr[j+1]=key;
	}
}
void array(int n,int arr[])
{
	int i;
	for(i=0;i<n;i++)
	{
		printf("%d",arr[i]);
		printf("\n");
	}
}
int main()
{
	int arr[]={9,7,0,6,5,4};
	int n=sizeof(arr)/sizeof(arr[0]);
	insertionsort(n,arr);
	array(n,arr);
	
	return 0;
}
